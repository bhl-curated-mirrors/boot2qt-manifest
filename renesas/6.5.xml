<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <include name="base/kirkstone.xml"/>

  <!-- Boot to Qt vendor layer -->
  <project name="meta-boot2qt-renesas"
           remote="qt"
           revision="dev"
           upstream="dev"
           path="sources/meta-boot2qt-renesas">
    <linkfile dest="sources/templates" src="conf"/>
  </project>

  <include name="qt/6.5.xml"/>

  <!-- Device specific layers -->
  <remote name="renesas"
          fetch="https://github.com/renesas-rcar"/>
  <project name="meta-renesas"
           remote="renesas"
           revision="644ccf213fed7be3cf68b2a7bd80ae32bcc8a3c8"
           upstream="kirkstone-dev"
           path="sources/meta-renesas"/>

  <!-- internal layer for the proprietary device drivers -->
  <remote name="playground"
          fetch="ssh://git@git.qt.io/boot2qt"/>
  <project name="renesas-rcar3"
           remote="playground"
           revision="79433791840724c3a47bc74ad6ae379e92d0a57a"
           upstream="v5.9.0-eval"
           path="sources/renesas-rcar3"
           groups="notdefault"/>
</manifest>
