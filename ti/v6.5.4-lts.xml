  <!-- Qt -->
  <project name="meta-qt6"
           remote="qt"
           revision="refs/tags/v6.5.4-lts"
           upstream="lts-6.5.4"
           path="sources/meta-qt6"/>

  <!-- Device specific layers -->
  <project name="meta-arm"
           remote="yocto"
           revision="68509775774d9cf304e2b422927f2a9636525155"
           upstream="mickledore"
           path="sources/meta-arm"/>
  <project name="meta-ti"
           remote="yocto"
           revision="b04409dcc386041822cb4f040dba70646e314553"
           upstream="master"
