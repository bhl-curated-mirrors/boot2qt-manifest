           revision="refs/tags/yocto-4.3.4"
           upstream="nanbield"
           path="sources/poky"/>
  <project name="meta-mingw"
           remote="yocto"
           revision="refs/tags/yocto-4.3.4"
           upstream="nanbield"
           path="sources/meta-mingw"/>

