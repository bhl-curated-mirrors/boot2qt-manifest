<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <include name="base/scarthgap.xml"/>

  <!-- Boot to Qt vendor layer -->
  <project name="meta-boot2qt-qcom"
           remote="qt"
           revision="dev"
           upstream="dev"
           path="sources/meta-boot2qt-qcom">
    <linkfile dest="sources/templates" src="conf/templates/default"/>
  </project>

  <include name="qt/dev.xml"/>

  <!-- Device specific layers -->
  <remote name="linaro"
          fetch="https://github.com/Linaro"/>
  <project name="meta-qcom"
           remote="linaro"
           revision="scarthgap"
           upstream="scarthgap"
           path="sources/meta-qcom"/>
</manifest>
