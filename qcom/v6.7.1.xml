<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <default upstream="nanbield"/>

  <!-- Yocto -->
  <remote name="yocto"
          fetch="git://git.yoctoproject.org"/>
  <project name="poky"
           remote="yocto"
           revision="refs/tags/yocto-4.3.3"
           upstream="nanbield"
           path="sources/poky"/>
  <project name="meta-mingw"
           remote="yocto"
           revision="refs/tags/yocto-4.3.3"
           upstream="nanbield"
           path="sources/meta-mingw"/>

  <!-- OpenEmbedded -->
  <remote name="oe"
          fetch="git://git.openembedded.org"/>
  <project name="meta-openembedded"
           remote="oe"
           revision="da9063bdfbe130f424ba487f167da68e0ce90e7d"
           upstream="nanbield"
           path="sources/meta-openembedded"/>

  <!-- Clang -->
  <remote name="kraj"
          fetch="https://github.com/kraj"/>
  <project name="meta-clang"
           remote="kraj"
           revision="b71a45630d19f210079f42f6394cdde65195fb84"
           upstream="nanbield"
           path="sources/meta-clang"/>

  <!-- Boot to Qt -->
  <remote name="qt"
          fetch="git://code.qt.io/yocto"
          pushurl="ssh://codereview.qt-project.org/yocto"/>
  <remote name="gerrit"
          fetch="ssh://codereview.qt-project.org/yocto"/>
  <project name="meta-boot2qt"
           remote="qt"
           revision="ab46faaeb9ee54b2f474c053fda6c370d1ec89b0"
           upstream="nanbield"
           path="sources/meta-boot2qt">
    <linkfile dest="setup-environment.sh" src="scripts/setup-environment.sh"/>
  </project>

  <!-- Boot to Qt vendor layer -->
  <project name="meta-boot2qt-qcom"
           remote="qt"
           revision="a1438348b6476fb69bdc84731a159e1a69d7fade"
           upstream="dev"
           path="sources/meta-boot2qt-qcom">
    <linkfile dest="sources/templates" src="conf/templates/default"/>
  </project>

  <!-- Qt -->
  <project name="meta-qt6"
           remote="qt"
           revision="refs/tags/v6.7.1"
           upstream="6.7.1"
           path="sources/meta-qt6"/>

  <!-- Device specific layers -->
  <remote name="linaro"
          fetch="https://github.com/Linaro"/>
  <project name="meta-qcom"
           remote="linaro"
           revision="e94335431ac0f6c37cd5edd3bdfc954adb79894d"
           upstream="master"
           path="sources/meta-qcom"/>
</manifest>
